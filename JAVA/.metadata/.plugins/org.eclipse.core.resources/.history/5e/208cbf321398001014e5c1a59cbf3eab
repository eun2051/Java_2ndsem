package mystyle;
import java.util.Scanner;

public class Ch6_4{

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int num;
		System.out.print("횟수를 입력하시오 : ");
		num= s.nextInt();
		
		for (int i = 1; i <= num; i++) {
			System.out.printf("%d X %d = %d\n", num, con, num * i);
		}
		s.close();
	}
}