package mystyle;
import java.util.Scanner;

public class Ch6_3{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int num;
		System.out.print("단를 입력하시오 : ");
		num= s.nextInt();
		
		for (int i = 1; i <= num; i++) {
			for(int j = 1; j <= 10; j++) {
				System.out.println( i + " * " + j + " = " + i * j);
			}
		}
		s.close();
	}
}