		num3= s.nextInt();
		System.out.print("단를 입력하시오 : ");
		num4= s.nextInt();
		
		for (int i = 1; i <= num; i++) {
			for(int j = 1; j <= num; j++) {
				System.out.println( i + " * " + j + " = " + i * j);
			}
		}
		s.close();
