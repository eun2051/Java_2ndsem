package mystyle;
import java.util.Scanner;

public class pro3{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int a_hp, a_hit;
		int b_hp, b_hit;
		System.out.print("플레이어 A의 HP를 입력하시오 : ");
		a_hp = s.nextInt();
		System.out.print("플레이어 A의 공격력을 입력하시오 : ");
		a_hit = s.nextInt();
		
		System.out.print("플레이어 B의 HP를 입력하시오 : ");
		b_hp = s.nextInt();
		System.out.print("플레이어 B의 공격력을 입력하시오 : ");
		b_hit = s.nextInt();
		
	}
}